\begin{table}[!hbt]
\begin{center}\label{transparencia tabla 1}
\begin{tabular}[c]{||l | l ||l|l||}
\hline
\hline
$x$ & $f(x)=\frac{1}{4x}$ &{\em Taylor} & Error \\
\hline
\rowcolor{LightCyan}
1 &0.25& 0.01875 & 25\\
\hline
1.5 &0.1666667&0.15625& 6.25\\
\hline
2 &0.125 &0.125 & 0 \\
\hline
2.5 &0.1 &0.09375 & 6.25 \\
\hline
3 & 0.08333 & 0.0625 & 25 \\
\hline
\hline
\end{tabular}
\caption{La $c$ vale 2 y está interpolada en grado 1}
\end{center}
\end{table}
